	<subDataset name="Dataset1" uuid="f353a876-26e0-4ce3-b095-075603a19c32">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="ArgusDB"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="codCurriculo" class="java.lang.String">
			<defaultValueExpression><![CDATA[$P{codCurriculo}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT ano as Série, SUM( cargaHoraria) AS Cargahorária
FROM argus.`curriculo-disciplina`
WHERE 
	 argus.`curriculo-disciplina`.`codCurriculo` = $P{codCurriculo}
GROUP BY argus.`curriculo-disciplina`.ano
ORDER BY argus.`curriculo-disciplina`.ano ASC]]>
		</queryString>
		<field name="Série" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="Série"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="curriculo-disciplina"/>
		</field>
		<field name="Cargahorária" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="Cargahorária"/>
		</field>
	</subDataset>
