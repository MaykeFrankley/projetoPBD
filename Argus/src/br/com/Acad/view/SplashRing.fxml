<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<VBox alignment="BOTTOM_CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.Acad.controller.SplashController">
   <children>
      <StackPane>
         <children>
            <ImageView fitHeight="595.0" fitWidth="848.0" pickOnBounds="true">
               <image>
                  <Image url="@../../../../../Resources/images/argus_logo.png" />
               </image>
               <effect>
                  <BoxBlur height="2.0" iterations="2" width="2.0" />
               </effect>
            </ImageView>
            <Circle fx:id="c1" fill="TRANSPARENT" radius="193.0" stroke="RED" strokeType="INSIDE" strokeWidth="20.0" style="-fx-stroke-dash-array: 80;" />
            <Circle fx:id="c2" fill="TRANSPARENT" radius="145.0" stroke="#111010" strokeType="INSIDE" strokeWidth="18.0" style="-fx-stroke-dash-array: 80;">
               <effect>
                  <Glow />
               </effect></Circle>
            <Circle fx:id="c3" fill="TRANSPARENT" radius="109.0" stroke="#6b6b6b" strokeType="INSIDE" strokeWidth="16.0" style="-fx-stroke-dash-array: 80;">
               <effect>
                  <Glow />
               </effect></Circle>
            <Circle fx:id="c4" fill="TRANSPARENT" radius="65.0" stroke="RED" strokeType="INSIDE" strokeWidth="14.0" style="-fx-stroke-dash-array: 50;">
               <effect>
                  <Glow level="1.0" />
               </effect></Circle>
         </children>
      </StackPane>
   </children>
   <effect>
      <Glow level="1.0" />
   </effect>
</VBox>
